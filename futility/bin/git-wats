#!/usr/bin/env bash
refs=($(git seq "$@"))
ref_start="${refs[0]}"
ref_end="${refs[1]}"
echo "File changes in "${ref_start}.."${ref_end}"
for ref in $(git log --pretty='format:%h' "${ref_start}..${ref_end}"); do
	echo
	git log --pretty=oneline -n1 "${ref}"
	git diff --name-status "${ref}^..${ref}"
done
