{
  "title": "Disable Dumb Mac Stuff",
  "rules": [
    {
        "description": "Disable Command+Shift+I email",
        "manipulators": [
            {
                "from": {
                    "key_code": "i",
                    "modifiers": {
                        "mandatory": [
                            "command",
                            "shift"
                        ],
                        "optional": []
                    }
                },
                "type": "basic"
            }
        ]
    },
    {
      "description": "Disable Command+M minimize",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "m",
            "modifiers": {
              "mandatory": [
                "command"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          }
        }
      ]
    },
    {
      "description": "Disable Cmd+Ctrl+D dictionary",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "d",
            "modifiers": {
              "mandatory": [
                "command",
                "control"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          }
        }
      ]
    },
    {
      "description": "Copy on print_screen",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "print_screen",
            "modifiers": {
              "mandatory": [],
              "optional": []
            },
            "to": {
              "key_code": "c",
              "modifiers": [
                "command"
              ]
            }
          }
        }
      ]
    },
    {
      "description": "Pasta on pause",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "pause",
            "modifiers": {
              "mandatory": [],
              "optional": []
            },
            "to": {
              "key_code": "v",
              "modifiers": [
                "command"
              ]
            }
          }
        }
      ]
    },
    {
      "description": "Disable Cmd+H hide",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "h",
            "modifiers": {
              "mandatory": [
                "command"
              ],
              "optional": [
                "caps_lock",
                "shift"
              ]
            }
          }
        }
      ]
    },
    {
      "description": "Eject is Sys Pref",
      "manipulators": [
        {
          "from": {
            "key_code": "eject",
            "modifiers": {}
          },
          "type": "basic",
          "to": {
            "shell_command": "open -a 'System Preferences'"
          }
        }
      ]
    },
    {
      "description": "Make Terminal Ctrl+K soft-clear",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": [
                "command"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.apple\\.Terminal$"
              ]
            }
          ],
          "to": [
            {
              "key_code": "c"
            },
            {
              "key_code": "l"
            },
            {
              "key_code": "e"
            },
            {
              "key_code": "a"
            },
            {
              "key_code": "r"
            },
            {
              "key_code": "return_or_enter"
            }
          ]
        }
      ]
    },
    {
      "description": "Disable Terminal Cmd+Left window swap",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "left_arrow",
            "modifiers": {
              "mandatory": [
                "command"
              ],
              "optional": []
            }
          },
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.apple\\.Terminal$"
              ]
            }
          ],
          "to": []
        }
      ]
    },{
      "description": "Disable Terminal Cmd+Right window swap",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "right_arrow",
            "modifiers": {
              "mandatory": [
                "command"
              ],
              "optional": []
            }
          },
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.apple\\.Terminal$"
              ]
            }
          ],
          "to": []
        }
      ]
    },
    {
      "description": "Log date",
      "manipulators": [
        {
          "from": {
            "key_code": "escape",
            "modifiers": {
              "mandatory": [
                "left_command"
              ]
            }
          },
          "type": "basic",
          "to": {
            "shell_command": "date >> ~/Desktop/open_editor.txt"
          }
        }
      ]
    },
    {
      "description": "Open editor",
      "manipulators": [
        {
          "from": {
            "key_code": "f1",
            "modifiers": {
              "mandatory": []
            }
          },
          "type": "basic",
          "to": {
            "shell_command": "bash -l edit"
          }
        }
      ]
    }
  ]
}